<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="iotdf.iotgateway">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DevicePanel" />
        <activity android:name=".Regist" />
        <activity android:name=".ChooseDevice" />

        <service
            android:name=".ConServ.Myservice"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".ConServ.ConSetting" />
        <activity android:name=".data.DataTest"></activity>
    </application>

</manifest>